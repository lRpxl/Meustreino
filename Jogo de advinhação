import random
import math

lower = int(input("digite um numero menor ="))
upper = int(input("digite um numero maior ="))

x = random.randint(lower, upper)
print("\n\tVocê só", round(math.log(upper - lower + 1 ,2)),"chance de Advinha o número !\n")

count = 0

while count <math.log(upper - lower +1, 2):
      count += 1
      
      advinha = int(input("Advinhe o número =")
      
      if x == advinha
      print("PARANBÉNS VOCÊ CONSEGUIU ADVINHA O NUMERO" , x, "jogue novamente !")
      
      break
      
   elif x > advinha:
      print('Você pensou pensou pequeno !!!')
      
   elif x < advinha
      print('Você pensou alto !!!')
      if count >=math.log(upper - lower +1, 2):
       print('O número é', x , 'Tente uma outra vez !!')
